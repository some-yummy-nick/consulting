{
  "name": "webpack-config",
  "author": "Marat Shaymardanov",
  "private": true,
  "url": "git://github.com/yeffasol/webpack.git",
  "description": "webpack starter project",
  "scripts": {
    "start": "set NODE_ENV=development&&webpack-dev-server --env development",
    "build": "set NODE_ENV=production&&npm run clean&&webpack --env production",
    "sprite": "svg-sprite-generate -d ./source/images/svg/ -o ./source/images/sprite.svg",
    "clean": "rimraf ./build/*",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-stage-0": "latest",
    "copy-webpack-plugin": "latest",
    "css-loader": "latest",
    "css-mqpacker": "latest",
    "extract-text-webpack-plugin": "^2.0.0",
    "favicons-webpack-plugin": "latest",
    "file-loader": "latest",
    "fontawesome-scss": "^2.0.4",
    "gh-pages": "latest",
    "glob-all": "latest",
    "html-webpack-plugin": "latest",
    "image-webpack-loader": "latest",
    "node-sass": "latest",
    "object-fit-images": "latest",
    "postcss-assets": "latest",
    "postcss-color-rgba-fallback": "latest",
    "postcss-easysprites": "latest",
    "postcss-filter-gradient": "latest",
    "postcss-inline-svg": "latest",
    "postcss-line-height-px-to-unitless": "latest",
    "postcss-loader": "latest",
    "postcss-object-fit-images": "latest",
    "purify-css": "latest",
    "purifycss-webpack": "latest",
    "rimraf": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "stylefmt": "latest",
    "stylefmt-loader": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest",
    "uglifyjs-webpack-plugin": "latest",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "latest"
  },
  "browserslist": [
    "last 10 versions",
    ">5%",
    "IE 10"
  ],
  "dependencies": {
    "jquery": "^2.2.4"
  }
}
